{
  "name": "@safe3devs/safedeploy",
  "version": "1.0.0",
  "description": "Universal Safe3Devs QR Deploy SDK for TypeScript and JavaScript with ethers.js and viem support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "contracts",
    "scripts",
    "plugins",
    "tests"
  ],
  "scripts": {
    "build": "npm run build:core && npm run build:ethers && npm run build:viem && npm run build:js && npm run build:plugin",
    "build:core": "tsc -p tsconfig.core.json",
    "build:ethers": "tsc -p tsconfig.ethers.json",
    "build:viem": "tsc -p tsconfig.viem.json",
    "build:js": "rollup -c rollup.config.js",
    "build:plugin": "tsc -p tsconfig.plugin.json",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "web3",
    "ethereum",
    "smart-contracts",
    "deployment",
    "walletconnect",
    "qr-code",
    "ethers",
    "viem",
    "hardhat",
    "typescript",
    "javascript"
  ],
  "author": "Safe3Devs",
  "license": "MIT",
  "dependencies": {
    "@walletconnect/sign-client": "^2.8.0",
    "@walletconnect/types": "^2.8.0",
    "qrcode-terminal": "^0.12.0",
    "events": "^3.3.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@types/qrcode-terminal": "^0.12.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "hardhat": "^2.19.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/safe3devs/safedeploy-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/safe3devs/safedeploy-sdk/issues"
  },
  "homepage": "https://github.com/safe3devs/safedeploy-sdk#readme"
}
